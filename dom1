<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>Document</title>
</head>
<body>
	
	<script>
		var num = window.prompt("请输入XXX的成绩!","");
		num *=1;//window.prompt方法始终只返回字符串,用这样的方法将其转换成数字
		// num = parseInt(num);
		if (isNaN(num)) {//不能将其它值转换成数字时将返回NaN,可以用内置的isNaN判断值是否是NaN
			alert("您输入的不是一个数字!");
		} else {
			if (num<=100 && num>=90) {
				alert("Excellent!");
			} else {
				if (num <90 && num>=80) {
					alert("Good!");
				} else {
					if (num < 80 && num >= 70) {
						alert("So so!");
					} else {
						if (num < 70 && num >=60) {
							alert("Be careful !!!");
						} else {
							if (num < 60 && num >= 0) {
								alert("Oh, NO!");
							} else {
								alert("USB!");
							}
						}
					}
				}
			}	
		}

	</script>
</body>
</html>
